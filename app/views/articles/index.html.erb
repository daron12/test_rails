<h1>Viewing all articles</h1>
<h1><%= link_to('Create new article ',new_article_path) %></h1>
<table border="1">
	<tr>
		<th>
			Title
		</th>
		<th>
			Description
		</th>
	</tr>
	

<% @articles.each do |article| %>
<tr>
	<td><%=article.title%></td>
	<td><%=article.description%></td>
	<td><%= link_to 'Edit',edit_article_path(article) %></td>
	<td><%= link_to 'Show',article_path(article) %></td>
</tr>


<%end%>
</table>

